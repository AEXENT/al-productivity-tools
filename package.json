{
  "name": "al-productivity-tools",
  "displayName": "AL Productivity Tools",
  "description": "Microsoft Business Central - AL Productivity Tools",
  "publisher": "MSNRaju",
  "version": "1.0.2",
  "icon": "icon/logo.png",
  "repository": {
    "url": "https://github.com/msnraju/al-productivity-tools"
  },
  "engines": {
    "vscode": "^1.47.0"
  },
  "categories": [
    "Linters",
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "onCommand:alProductivityTools.standardizeALCode",
    "onCommand:alProductivityTools.standardizeALFiles",
    "onCommand:alProductivityTools.fixAlFileNames"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "alProductivityTools.standardizeALCode",
        "title": "MSN: Standardize AL Code"
      },
      {
        "command": "alProductivityTools.standardizeALFiles",
        "title": "MSN: Standardize AL Files"
      },
      {
        "command": "alProductivityTools.fixAlFileNames",
        "title": "MSN: Fix AL File Names"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.157",
    "@types/mocha": "^7.0.2",
    "@types/n-readlines": "^1.0.1",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.47.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "lodash": "^4.17.19",
    "n-readlines": "^1.0.0"
  },
  "keywords": [
    "Microsoft",
    "AL",
    "Naming Conventions",
    "Business Central",
    "Dynamics 365",
    "Tools",
    "Productivity"
  ]
}
